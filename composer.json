{
    "name": "fntneves/laravel-transactional-events",
    "description": "Transactional layer for Laravel Event Dispatcher",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "laravel/framework": "5.5.* || 5.6.*"
    },
    "authors": [
        {
            "name": "Francisco Neves",
            "email": "hi@francisconeves.me"
        }
    ],
    "autoload": {
        "psr-0": {
            "Neves\\Events\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neves\\Events\\EventServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
