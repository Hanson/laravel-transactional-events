    "type": "library",
    "require": {
        "illuminate/events": "5.5.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "friendsofphp/php-cs-fixer": "^2.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Neves",
            "email": "contact@francisconeves.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Neves\\Events\\": "src/"
        }
    },
    "extra": {
